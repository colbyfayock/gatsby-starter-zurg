{
  "name": "gatsby-starter-zurg",
  "description": "A Gatsby starter with Sass and no assumptions",
  "version": "0.1.1",
  "author": "Colby Fayock <hello@colbyfayock.com>",
  "dependencies": {
    "gatsby": "^2.3.31",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.32",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@fabiospampinato/bump": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-gatsby": "^0.1.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf public/* && gatsby build",
    "bump": "bump",
    "develop": "gatsby develop",
    "lint": "yarn lint-js && yarn lint-css",
    "lint-css": "stylelint 'src/assets/stylesheets/**/*.scss' --config ./config/.stylelintrc",
    "lint-js": "eslint -c ./config/.eslintrc . --ext .js --ignore-path ./config/.eslintignore",
    "format": "yarn format-js && yarn format-css",
    "format-js": "prettier-eslint --eslint-config-path ./config/.eslintrc --write '**/*.js'",
    "format-css": "yarn lint-css --fix",
    "precommit": "lint-staged",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest --config ./config/jest.config.js",
    "test-watch": "yarn test --watchAll"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "yarn format-js",
        "yarn lint-js",
        "git add"
      ],
      "**/*.scss": [
        "yarn format-css",
        "yarn lint-css",
        "git add"
      ]
    },
    "ignore": [
      ".cache",
      "coverage",
      "node_modules",
      "public"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colbyfayock/gatsby-starter-zurg"
  },
  "bugs": {
    "url": "https://github.com/colbyfayock/gatsby-starter-zurg/issues"
  }
}
